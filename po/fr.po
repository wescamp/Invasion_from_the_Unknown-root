msgstr "bâton"

#: Invasion_from_the_Unknown/units/humans/Aragwaith_Warlock.cfg:40
msgid "fireball"
msgstr "boule de feu"

#: Invasion_from_the_Unknown/units/humans/Aragwaith_Warlock.cfg:114
msgid "female^Warlock"
